{
  "name": "@ng-apimock/cypress-plugin",
  "description": "Cypress plugin for ng-apimock",
  "version": "2.2.0",
  "homepage": "https://github.com/ng-apimock/cypress-plugin#readme",
  "author": {
    "name": "Mischa Dasberg",
    "email": "mischa@dasberg.nl"
  },
  "main": "dist/index.js",
  "types": "dist/cypress.d.ts",
  "scripts": {
    "cleanup": "node test/cleanup.js",
    "precompile": "rm -rf dist",
    "compile": "tsc -p tsconfig.json",
    "postcompile": "copyfiles -u 1 src/**/*.d.ts dist",
    "lint": "eslint \"src/**/*.ts\"",
    "postlint": "eslint \"test/**/*.js\" --rule \"jest/valid-expect: off\" --rule \"import/named: off\"",
    "lint:fix": "npm run lint -- --fix",
    "start": "node test/server.js",
    "test": "jest --config=jest.config.js -w 1",
    "cypress-local": "cypress open"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ng-apimock/cypress-plugin.git"
  },
  "bugs": {
    "url": "https://github.com/ng-apimock/cypress-plugin/issues"
  },
  "licenses": [
    {
      "type": "MIT",
      "url": "https://github.com/ng-apimock/cypress-plugin/blob/master/LICENSE-MIT"
    }
  ],
  "keywords": [
    "apimock",
    "ng-apimock",
    "http",
    "api",
    "cypress",
    "plugin"
  ],
  "devDependencies": {
    "@commitlint/cli": "15.0.0",
    "@commitlint/config-conventional": "15.0.0",
    "@ng-apimock/core": "3.4.0",
    "@ng-apimock/test-application": "3.3.0",
    "@semantic-release/changelog": "6.0.1",
    "@semantic-release/commit-analyzer": "9.0.2",
    "@semantic-release/git": "10.0.1",
    "@semantic-release/github": "8.0.2",
    "@semantic-release/npm": "8.0.3",
    "@semantic-release/release-notes-generator": "10.0.3",
    "@types/bluebird": "3.5.36",
    "@types/chai": "4.2.22",
    "@types/connect": "3.4.35",
    "@types/fs-extra": "9.0.13",
    "@types/jest": "27.0.3",
    "@types/node": "16.11.11",
    "@types/url-join": "4.0.1",
    "@types/uuid": "8.3.3",
    "@typescript-eslint/eslint-plugin": "5.5.0",
    "@typescript-eslint/parser": "5.5.0",
    "bluebird": "3.7.2",
    "chai": "4.3.4",
    "commitizen": "4.2.4",
    "connect": "3.7.0",
    "copyfiles": "2.4.1",
    "cypress": "9.1.0",
    "cypress-cucumber-preprocessor": "4.3.0",
    "eslint": "8.3.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-plugin-cypress": "2.12.1",
    "eslint-plugin-import": "2.25.3",
    "eslint-plugin-jest": "25.3.0",
    "eventemitter2": "6.4.5",
    "express": "4.17.1",
    "fs-extra": "10.0.0",
    "http-proxy-middleware": "2.0.1",
    "husky": "7.0.4",
    "jest": "27.4.3",
    "jest-matchers": "20.0.3",
    "lint-staged": "12.1.2",
    "semantic-release": "18.0.1",
    "serve-static": "1.14.1",
    "ts-jest": "27.0.7",
    "ts-node": "10.4.0",
    "typescript": "4.6.3"
  },
  "dependencies": {
    "@ng-apimock/base-client": "3.1.0",
    "urljoin": "0.1.5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "**/**.{ts}": [
      "eslint --fix"
    ]
  },
  "peerDependencies": {
    "cypress": ">=4"
  },
  "cypress-cucumber-preprocessor": {
    "step_definitions": "test/step_definitions"
  }
}
